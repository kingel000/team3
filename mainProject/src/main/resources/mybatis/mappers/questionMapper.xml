<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="main.project.web.question.dao.IQuestionDAO">
		<select id="insertQuestion" parameterType="QuestionVO" resultType="questionVO">
			INSERT INTO BOARD_QUESTION VALUES 
			(#{BOARD_QUESTION_NUM}, #{BOARD_QUESTION_TITLE},
			 #{BOARD_QUESTION_INFO},#{BOARD_QUESTION_FILE},
			 #{BOARD_QUESTION_DATE},#{BOARD_QUESTION_WRITER})

		</select>
		<select id="deleteQuestion" parameterType="questionVO" >
			DELETE FROM board_question WHERER 
			board_question_num=#{board_question_num}
		</select>
		<select id="selectQuestion" parameterType="questionVO" resultType="questionVO">
			SELECT * FROM board_question WHERER 
			board_question_num=#{board_question_num}
		</select>
		<select id="selectiListQuestion" resultType="questionVO">
			SELECT * FROM board_question
		</select>
	
</mapper>
